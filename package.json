{
  "name": "5h-engine",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "@types/jest": "^19.2.2",
    "jest": "^19.0.2",
    "ts-jest": "^19.0.10",
    "typescript": "rc"
  },
  "scripts": {
    "test": "jest --watchAll",
    "build": "tsc"
  },
  "jest": {
    "watch": true,
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "src/.*test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
